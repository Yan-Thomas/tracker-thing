{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": [
                "**/*",
                "!test/**/*"
            ],
            "outputs": [
                "dist/**/*"
            ],
            "outputLogs": "new-only"
        },
        "dev": {
			"cache": false,
			"persistent": true
		}
    }
}